{
    "bundles": [
        {
            "id": "org.apache.sling:org.apache.sling.commons.thumbnails:${project.version}",
            "start-order": "20"
        }
    ],
    "configurations":{
        "org.apache.sling.serviceusermapping.impl.ServiceUserMapperImpl.amended~sling-commons-thumbnails":{
            "user.mapping":[
                "org.apache.sling.commons.thumbnails:sling-commons-thumbnails=[sling-commons-thumbnails]"
            ]
        }
    },
    "repoinit:TEXT|true": [
        "create service user sling-commons-thumbnails with path system/sling",
        "set principal ACL for sling-commons-thumbnails",
        "allow   jcr:write,jcr:nodeTypeManagement,jcr:versionManagement    on /content",
        "allow   jcr:read    on /",
        "end"
        
    ]
}
